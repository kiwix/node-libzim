{
  "name": "@openzim/libzim",
  "version": "2.0.14",
  "description": "Libzim bindings for NodeJS",
  "scripts": {
    "clean": "rm -rf dist build/native/build",
    "autogypi": "autogypi",
    "node-gyp": "node-gyp",
    "emcc-path": "emcc-path",
    "copyasm": "copyasm",
    "ndts": "ndts",
    "tsc": "tsc",
    "build:native": "autogypi -r build && node-gyp -C build/native configure build && copyasm build/native dist",
    "install": "npm run download && (npm run build:native || echo Warning: problems with native build)",
    "download": "node ./download-libzim.js",
    "prepublish": "tsc && npm run build:native",
    "test": "node dist/test.js",
    "test-mem-leak": "tsc && node dist/makeLargeZim.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "main": "dist/index.js",
  "author": "Joseph Reeve",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openzim/node-libzim.git"
  },
  "bugs": {
    "url": "https://github.com/openzim/node-libzim/issues"
  },
  "homepage": "https://github.com/openzim/node-libzim#readme",
  "dependencies": {
    "@types/faker": "^4.1.5",
    "@types/mime": "^2.0.1",
    "@types/node": "^10.12.21",
    "@types/rimraf": "^2.0.2",
    "autogypi": "^0.2.2",
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "exec-then": "^1.3.1",
    "faker": "^4.1.0",
    "mime": "^2.4.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "nbind": "^0.3.6",
    "node-gyp": "^3.3.1",
    "rimraf": "^2.6.3",
    "tar": "^4.4.8",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "snyk": "^1.161.1"
  },
  "devDependencies": {
    "typescript": "^2.1.4"
  },
  "snyk": true
}
